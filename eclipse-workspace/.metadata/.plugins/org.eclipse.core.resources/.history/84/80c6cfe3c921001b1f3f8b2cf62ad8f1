package testandoPoo;

public class Compra {
	
	String item;
	int quantidade;
	double preco;
	double total;
	String tos;
	
	Compra(String item, int quantidade, double preco){
		this.item = item;
		this.quantidade  = quantidade;
		this.preco  = preco;
	}
	
	public String toString() {
		
		tos = String.valueOf(this.quantidade)+ "->" +this.item;
		return tos;
	}
	

	
}
