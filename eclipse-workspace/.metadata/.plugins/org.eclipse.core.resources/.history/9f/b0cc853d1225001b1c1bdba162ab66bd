package desafio_poo;

public class Civic extends Carro{

	double fatorAcelerar = 3.25;
	double fatorFrear = 2.5;
	double velocidade = 1;
	
	@Override
	void acelerar() {
		// TODO Auto-generated method stub
		if(status = true ) {this.velocidade += this.fatorAcelerar;}
	}
	@Override
	void frear() {
		// TODO Auto-generated method stub
		super.frear();
	}
	
	
	
	
}
