package testandoPoo;

public class POO1 {
	public static void main(String[] args) {
		
		Cliente c1  = new Cliente("Felipe");
		Cliente c2 = new Cliente ("Maria");
		Cliente c3 = new Cliente ("Valmir");
		
		c1.compras.add(new Compra("Carrinho", 3, 8.89));
		c1.compras.add(new Compra("Jarro", 2 , 3.89));
		
		System.out.println(c1.compras.toString());
		
	}
}
